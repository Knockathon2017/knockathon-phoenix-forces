@model List<GreenCredits.DAL.CarbonAsset>
@{
    Layout = null;

}
<!DOCTYPE html>
<html>
<head>
    <title>GreenCredits</title>
    <link href="/content/styles/styles.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans|Josefin+Sans' rel='stylesheet' type='text/css'>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="~/content/Scripts/dashboard.js"></script>
    <script src='https://api.mapbox.com/mapbox.js/v2.2.2/mapbox.js'></script>
    <link href='https://api.mapbox.com/mapbox.js/v2.2.2/mapbox.css' rel='stylesheet' />
</head>
<body>
    <div id="fb-root"></div>
    <script>
(function (d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_GB/sdk.js#xfbml=1&version=v2.4";
    fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));
    </script>

    <a href="https://twitter.com/apegpbsk" ; class="twitter-follow-button" data-show-count="false" data-show-screen-name="false">Follow us</a>
    <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>


    <div class="fb-like" data-href="https://www.greencredits.com" data-width="20px" data-layout="button_count" data-action="like" data-show-faces="true" data-share="true"></div>
    <div class="header">
        <a href="/home" class="logo">GreenCredits<sup>TM</sup><img src="/content/images/logo_part.png" /></a>
        <ul class="top_nav">
            <li>Welcome @ViewBag.UserFullName</li>
            <li><a href="/logout">Logout</a></li>
        </ul>
    </div>
    <div class="dashboard_section">
        <div class="inside_unit">
            @*<ul class="dashboard_nav">
                    <li><a href="#" class="active">Assets</a></li>
                    <li><a href="#">Photo Gallery</a></li>
                    <li><a href="#">Profile</a></li>
                </ul>*@
            <div class="content_container">
                <div class="heading">Assets Listing <a id="addAssetsBtn" href="javascript:void(0);">Add New</a></div>
                <div id="addContainer" class="popover_ctr" style="display:none;">
                    <span class="arrow"></span>
                    <form id="form_add" class="form_container">
                        <div class="row_block">
                            <label>Location</label>
                            <input type="text" placeholder="Address Line" />
                            <input type="text" class="small_input" placeholder="State" />
                            <input type="text" class="small_input" placeholder="City" />
                            <input type="text" class="small_input" placeholder="District" />
                        </div>
                        <div class="row_block">
                            <label>No. of trees</label>
                            <input type="text" placeholder="No. of trees" />
                        </div>
                        <div class="row_block">
                            <a id="saveAssets" href="javascript:void(0);" class="button">Save</a> or <a id="cancelAssets" href="javascript:void(0);">Cancel</a>
                        </div>

                    </form>
                </div>

                <table class="grid_container" cellpadding="0" cellspacing="0">
                    <tr class="head">
                        <td>Location</td>
                        <td>No. of trees</td>
                        <td>Organization</td>
                        <td>Passive trees</td>
                        <td>&nbsp;</td>
                    </tr>
                    @foreach (var item in Model)
                    {


                        <tr>

                            <td>@item.address</td>
                            <td>@item.activetreecount</td>
                            <td>@item.companyname</td>
                            <td>--</td>
                            <td><a href="#">Edit</a> <a href="#">Delete</a></td>
                        </tr>
                        <tr>

                            <td>@item.address</td>
                            <td>--</td>
                            <td>--</td>
                            <td>@item.passivetreecount</td>
                            <td><a href="#">Edit</a> <a href="#">Delete</a></td>
                        </tr>
                    }

                </table>

                <div class="stats_container">
                    2000INR<span class="help_txt">earned so far</span>
                </div>

                <div class="heading" style="display:none;">Photo Gallery <a href="#">Upload New Photos</a></div>

                <div class="gallery_carousel" style="display:none;">
                    <img src="/content/images/gallery1.png" />
                    <img src="/content/images/gallery2.png" />
                    <img src="/content/images/gallery3.png" />
                    <img src="/content/images/gallery4.png" />
                    <img src="/content/images/gallery1.png" />
                    <img src="/content/images/gallery2.png" />
                    <img src="/content/images/gallery3.png" />
                    <img src="/content/images/gallery4.png" />
                    <img src="/content/images/gallery1.png" />
                    <img src="/content/images/gallery2.png" />
                    <img src="/content/images/gallery3.png" />
                    <img src="/content/images/gallery4.png" />
                </div>
                <div id='map' style="width:100%;height:400px;"></div>
            </div>

            <div class="content_container">
                <div class="heading">Update Profile</div>

                <form class="form_container">
                    <div class="row_block">
                        <label>Full Name</label>
                        <input type="text" placeholder="Full Name" />
                    </div>
                    <div class="row_block">
                        <label>Date of Birth</label>
                        <input type="text" placeholder="Date of Birth" />
                    </div>
                    <div class="row_block">
                        <label>Mobile</label>
                        <input type="text" placeholder="Mobile" />
                    </div>

                    <div class="row_block">
                        <label>Landline</label>
                        <input type="text" placeholder="Landline" />
                    </div>

                    <div class="row_block">
                        <label>Address</label>
                        <input type="text" placeholder="Address Line" />
                        <input type="text" class="small_input" placeholder="State" />
                        <input type="text" class="small_input" placeholder="City" />
                        <input type="text" class="small_input" placeholder="District" />
                    </div>
                    <div class="row_block">
                        <a href="dashboard.html" class="button">Update</a>
                    </div>
                </form>
            </div>




            <div class="clr"></div>
        </div>
    </div>

    <div class="copyright_section">
        &copy GreenCredits 2015
    </div>
    <script id="displayAssets" type="text/x-jquery-tmpl">
        <tr>
            <td>${address}</td>
            <td>${activetreecount}</td>
            <td>${Companyname}</td>
            @*<td><a href="javascript:void(0);">Edit</a> <a href="javascript:void(0);">Delete</a></td>*@
        </tr>
    </script>
    <script>
        $(document).ready(function () {
            var objDashboard = new Dashboard();
        });
    </script>
</body>
</html>>
