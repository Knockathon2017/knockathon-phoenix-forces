@model List<GreenCredits.DAL.CarbonAsset>
@{
    Layout = null;
    
}
<!DOCTYPE html>
<head>
    <title>GreenCredits</title>
    <link href="/content/styles/styles.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans|Josefin+Sans' rel='stylesheet' type='text/css'>
    <body>
        <div class="header">
            <a href="index.html" class="logo">GreenCredits<img src="/content/images/logo_part.png" /></a>
            <ul class="top_nav">
                <li><a href="/logout">Logout</a></li>
            </ul>
        </div>
        <div class="dashboard_section">
            <div class="inside_unit">
                <ul class="dashboard_nav">
                    <li><a href="#" class="active">Assets</a></li>
                    <li><a href="#">Photo Gallery</a></li>
                    <li><a href="#">Profile</a></li>
                </ul>
                <div class="content_container">
                    <div class="heading">Assets Listing <a href="#">Add New</a></div>

                    <table class="grid_container" cellpadding="0" cellspacing="0">
                        <tr class="head">
                            <td>Location</td>
                            <td>No. of trees</td>
                            <td>Organization</td>
                            <td>&nbsp;</td>
                        </tr>
                        @foreach (var item in Model)
	                        {
		 
	
                        <tr>
                      
                            <td>@item.address</td>
                            <td>@item.activetreecount</td>
                            <td>@item.Companyname</td>
                            <td><a href="#">Edit</a> <a href="#">Delete</a></td>
                        </tr>
                        }
                        
                    </table>

                </div>

                <div class="content_container">
                    <div class="heading">Assets Report <a href="#">Add New</a></div>

                    <table class="grid_container" cellpadding="0" cellspacing="0">
                        <tr class="head">
                            <td>Asset Name</td>
                            <td>Location</td>
                            <td>No. of trees</td>
                            <td>Organization</td>
                            <td>&nbsp;</td>
                        </tr>
                        <tr>
                            <td>Some Name</td>
                            <td>Noida</td>
                            <td>50</td>
                            <td>ABC</td>
                            <td><a href="#">Edit</a> <a href="#">Delete</a></td>
                        </tr>
                    </table>

                </div>

                <div class="clr"></div>

                <div class="content_container">
                    <div class="heading">Assets Report <a href="#">Add New</a></div>

                    <table class="grid_container" cellpadding="0" cellspacing="0">
                        <tr class="head">
                            <td>Asset Name</td>
                            <td>Location</td>
                            <td>No. of trees</td>
                            <td>Organization</td>
                            <td>&nbsp;</td>
                        </tr>
                        <tr>
                            <td>Some Name</td>
                            <td>Noida</td>
                            <td>50</td>
                            <td>ABC</td>
                            <td><a href="#">Edit</a> <a href="#">Delete</a></td>
                        </tr>
                    </table>

                </div>


                <div class="clr"></div>
            </div>
        </div>

        <div class="copyright_section">
            &copy GreenCredits 2015
        </div>

    </body>
</head>
