@model List<GreenCredits.DAL.CarbonAsset>
@{
    Layout = null;
    
}
<!DOCTYPE html>
<head>
    <title>GreenCredits</title>
    <link href="/content/styles/styles.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans|Josefin+Sans' rel='stylesheet' type='text/css'>
    <body>
        <div class="header">
            <a href="index.html" class="logo">GreenCredits<img src="/content/images/logo_part.png" /></a>
            <ul class="top_nav">
                <li><a href="/logout">Logout</a></li>
            </ul>
        </div>
        <div class="dashboard_section">
            <div class="inside_unit">
                @*<ul class="dashboard_nav">
                    <li><a href="#" class="active">Assets</a></li>
                    <li><a href="#">Photo Gallery</a></li>
                    <li><a href="#">Profile</a></li>
                </ul>*@
                <div class="content_container">
                    <div class="heading">Assets Listing <a href="#">Add New</a></div>

                    <table class="grid_container" cellpadding="0" cellspacing="0">
                        <tr class="head">
                            <td>Location</td>
                            <td>No. of trees</td>
                            <td>Organization</td>
                            <td>&nbsp;</td>
                        </tr>
                        @foreach (var item in Model)
	                        {
		 
	
                        <tr>
                      
                            <td>@item.address</td>
                            <td>@item.activetreecount</td>
                            <td>@item.Companyname</td>
                            <td><a href="#">Edit</a> <a href="#">Delete</a></td>
                        </tr>
                        }
                        
                    </table>

                    <div class="stats_container">
                        2000INR<span class="help_txt">earned so far</span>
                    </div>

                    <div class="heading">Photo Gallery <a href="#">Upload New Photos</a></div>

                    <div class="gallery_carousel">
                        <img src="/content/images/gallery1.png" />
                        <img src="/content/images/gallery2.png" />
                        <img src="/content/images/gallery3.png" />
                        <img src="/content/images/gallery4.png" />
                        <img src="/content/images/gallery1.png" />
                        <img src="/content/images/gallery2.png" />
                        <img src="/content/images/gallery3.png" />
                        <img src="/content/images/gallery4.png" />
                        <img src="/content/images/gallery1.png" />
                        <img src="/content/images/gallery2.png" />
                        <img src="/content/images/gallery3.png" />
                        <img src="/content/images/gallery4.png" />
                    </div>

                </div>

                <div class="content_container">
                    <div class="heading">Update Profile</div>

                    <form class="form_container">
                        <div class="row_block">
                            <label>Full Name</label>
                            <input type="text" placeholder="Full Name" />
                        </div>
                        <div class="row_block">
                            <label>Date of Birth</label>
                            <input type="text" placeholder="Date of Birth" />
                        </div>
                        <div class="row_block">
                            <label>Mobile</label>
                            <input type="text" placeholder="Mobile" />
                        </div>

                        <div class="row_block">
                            <label>Landline</label>
                            <input type="text" placeholder="Landline" />
                        </div>

                        <div class="row_block">
                            <label>Address</label>
                            <input type="text" placeholder="Address Line" />
                            <input type="text" class="small_input" placeholder="State" />
                            <input type="text" class="small_input" placeholder="City" />
                            <input type="text" class="small_input" placeholder="District" />
                        </div>

                        <div class="row_block">
                            <a href="dashboard.html" class="button">Update</a>
                        </div>

                    </form>

                    

                </div>

                <div class="clr"></div>

                


                <div class="clr"></div>
            </div>
        </div>

        <div class="copyright_section">
            &copy GreenCredits 2015
        </div>

    </body>
</head>
