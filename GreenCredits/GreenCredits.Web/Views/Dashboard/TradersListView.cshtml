@model List<GreenCredits.DAL.Traders>
<table class="grid_container" cellpadding="0" cellspacing="0">
    <tr class="head">
        <td>Company Name</td>
        <td>Address</td>
        <td>Product</td>
        <td>Web site</td>
    </tr>
    @foreach (var item in Model)
    {


        <tr>

            <td>@item.address</td>
            <td>@item.place</td>
            <td>@item.product</td>
            <td><a href="@item.website" target="_blank">@item.website</a></td>
            
        </tr>
       
    }

</table>

